{
  "name": "My Bookmarks",
  "version": "1.1",
  "app": {
    "background": {
      "scripts": ["background.js"]
    }
  },
  "description": "A browser action with a popup dump of all bookmarks, including search, add, edit and delete.",
  "permissions": [
    "bookmarks",
    "videoCapture"
  ],
  "browser_action": {
      "default_title": "My Bookmarks",
      "default_icon": "icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2
}
